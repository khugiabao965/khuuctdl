# include <stdio.h.>
# include <conio.h>

int main (){
	{
		int n ;
		printf ("nhap so phan tu  cua mang :");
		scanf ("%d",&n);
	
	
	}
	return 0;
	
}

// nhap mang 
void nhapmang (int mang[], int n){
	for (int i= 0; i< n; ++i) {
		printf(" nhap phan tu thu %d cua mang :", i);
		scanf ("%d ", &mang[i]);
		
	}
	
	
}  

// xuat mang
void xuatmang ( const int mang[], int n ){
	
	 printf (" mang da nhap  la ; ");
	 for ( int i=0; i< n; ++i){
	 	printf ("%d", mang[i]);
	
	 }
	 
	 
}

// tim kiem tuyeen tinh
void timkiemtuyentinh( const int mang[], int n ,int x);{
	int count = 0;
	printf (" tim kiem tuyen tinh :\n");
	for (int i = 0; i< n; ++i){
		if (mang[i]== x){
			count++;
			printf ("phan tu %d xuat hien o vi tri %d\n", x,i);
			
		}
	}
	it ( count == 0){
		printf (" khong tin thay phan tu %d trong mang.\n",x);
			}else {
				printf (" phan tu %d xuat hien %d  lan trong mang.\n",x count);
				
			}
	
			
}
